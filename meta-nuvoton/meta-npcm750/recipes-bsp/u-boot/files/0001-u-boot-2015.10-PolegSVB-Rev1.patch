+/*---------------------------------------------------------------------------------------------------------*/
+static void sdhci_activate_led(struct sdhci_host *host)
+{
+    u8 ctrl;
+
+    ctrl = sdhci_readb(host, SDHCI_HOST_CONTROL);
+    ctrl |= SDHCI_CTRL_LED;
+    sdhci_writeb(host, ctrl, SDHCI_HOST_CONTROL);
+}
